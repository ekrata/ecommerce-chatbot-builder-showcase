{
  "name": "echat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "sst bind next dev",
    "seed-dev": "sst bind ts-node populate.ts && sst bind next dev",
    "seed": "sst bind ts-node populate.ts",
    "dense-seed": "sst bind ts-node densePopulate.ts",
    "stripe-create": "sst bind --stage local ts-node packages/functions/app/api/src/stripe/initStripe.ts",
    "remove-local-db": "sst remove dbStack --stage local",
    "delete-local-table": "sst bind ts-node deleteTable.ts",
    "build": "sst bind next build",
    "dev-widget": "sst bind next dev './widget'",
    "build-widget": "next build './src/app/[locale]/(chat-widget)'",
    "publish-widget": "pnpm publish './src/app/[locale]/(chat-widget)'",
    "start": "sst bind next start",
    "sst-dev": "npx sst dev --stage local",
    "lint": "next lint",
    "sst build": "sst build",
    "timeout": "node -e \"setTimeout(() => process.exit(0), 10000)\" ",
    "timeout-long": "node -e \"setTimeout(() => process.exit(0), 30000)\" ",
    "timeout-seed": "npm-run-all timeout-long seed",
    "reset-local-db": "npm-run-all remove-local-db --parallel sst-dev timeout-seed",
    "test-app-api": "node -e \"setTimeout(() => process.exit(0), 10000)\" && sst bind 'vitest run --threads=false -c vitest/appApi.config.ts'",
    "test-app-ws": "node -e \"setTimeout(() => process.exit(0), 10000)\" && sst bind 'vitest run --threads=false -c vitest/appWs.config.ts'",
    "test-app-api-{articles}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts articles'",
    "run-test-app-api-{articles}": "npm-run-all -p sst-dev timeout test-app-api-{articles}",
    "test-app-api-{conversations}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts conversations'",
    "test-app-api-{customers}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts customers'",
    "test-app-api-{messages}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts messages'",
    "test-app-api-{visits}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts visits'",
    "test-app-api-{translations}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts translations'",
    "test-app-api-{configuration}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts configuration'",
    "test-app-api-{operators}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts operators'",
    "test-app-api-{ddb-stream}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts ddb-stream'",
    "test-app-api-{bots}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts bots'",
    "test-app-api-{bot-executions}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts botExecutions'",
    "test-app-api-{sales}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts sales'",
    "test-app-api-{chatBot}": "sst bind 'vitest packages/functions/app/api/src/nodes/chatbots/chat/chat.test.ts'",
    "test-app-api-{interactions}": "sst bind 'vitest run --threads=false -c vitest/appApi.config.ts interactions'",
    "test-app-ws-{messages}": "sst bind 'vitest run --threads=false -c vitest/appWs.config.ts messages'",
    "test-app-ws-{conversations}": "sst bind 'vitest run --threads=false -c vitest/appWs.config.ts conversations'",
    "test-app-ws-{customers}": "sst bind 'vitest run --threads=false -c vitest/appWs.config.ts customers'",
    "test-app-ws-{operators}": "sst bind 'vitest run --threads=false -c vitest/appWs.config.ts operators'",
    "test-app-ws-{visits}": "sst bind 'vitest run --threads=false -c vitest/appWs.config.ts visits'",
    "run-test-app-api-{conversations}": "npm-run-all -p sst-dev timeout test-app-api-{conversations}",
    "run-test-app-api-{customers}": "npm-run-all -p sst-dev timeout test-app-api-{customers}",
    "run-test-app-api-{messages}": "npm-run-all -p sst-dev timeout test-app-api-{messages}",
    "run-test-app-api-{visits}": "npm-run-all -p sst-dev timeout test-app-api-{visits}",
    "run-test-app-api-{translations}": "npm-run-all -p sst-dev timeout test-app-api-{translations}",
    "run-test-app-api-{operators}": "npm-run-all -p sst-dev timeout test-app-api-{operators}",
    "run-test-app-api-{ddb-stream}": "npm-run-all -p sst-dev timeout test-app-api-{ddb-stream}",
    "run-test-app-api-{bots}": "npm-run-all -p sst-dev timeout test-app-api-{bots}",
    "run-test-app-api-{bot-executions}": "npm-run-all -p sst-dev timeout test-app-api-{bot-executions}",
    "run-test-app-api-{interactions}": "npm-run-all -p sst-dev timeout test-app-api-{interactions}",
    "run-test-app-ws-{messages}": "npm-run-all -p sst-dev timeout test-app-api-{messages}",
    "run-test-app-ws-{conversations}": "npm-run-all -p sst-dev timeout test-app-api-{conversations}",
    "run-test-app-ws-{customers}": "npm-run-all -p sst-dev timeout test-app-api-{customers}",
    "run-test-app-api": "npm-run-all -p sst-dev test-app-api",
    "run-test-app-ws": "npm-run-all -p sst-dev test-app-ws",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-app-storybook": "test-storybook",
    "run-sb": "npm-run-all -p sst-dev storybook",
    "run-test-app-storybook": "npm-run-all -p storybook test-storybook",
    "test:fe": "jest --coverage",
    "test:single": "jest --watch --collectCoverage=false",
    "prepare": "husky install"
  },
